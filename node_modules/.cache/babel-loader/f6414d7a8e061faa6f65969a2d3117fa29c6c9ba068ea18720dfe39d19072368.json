{"ast":null,"code":"import ListContainer from './ListContainer';\nimport Recommend from './Recommend';\nimport Floor from './Floor';\nimport Brand from './Brand';\nimport { mapState } from 'vuex';\nexport default {\n  name: 'Home',\n  components: {\n    ListContainer,\n    Recommend,\n    Floor,\n    Brand\n  },\n  computed: {\n    // 从vuex中读取floorList（楼层数据）\n    ...mapState('home', ['floorList'])\n  },\n  mounted() {\n    // Home组件一挂载，就联系getFloorList获取楼层数据，并存入Vuex\n    this.$store.dispatch('home/getFloorList');\n  }\n};","map":{"version":3,"names":["ListContainer","Recommend","Floor","Brand","mapState","name","components","computed","mounted","$store","dispatch"],"sources":["src/pages/Home/index.vue"],"sourcesContent":["<template>\r\n\t<div>\r\n\t\t<!-- 三级分类导航 -->\r\n\t\t<TypeNav/>\r\n\t\t<!-- 列表 -->\r\n\t\t<ListContainer/>\r\n\t\t<!-- 今日推荐 -->\r\n\t\t<Recommend/>\r\n\t\t<!-- 楼层 -->\r\n\t\t<Floor v-for=\"(f,index) in floorList\" :key=\"f.id\" :floor=\"f\" :index=\"index\"/>\r\n\t\t<!-- 商标 -->\r\n\t\t<Brand/>\r\n\t</div>\r\n</template>\r\n\r\n<script>\r\n\timport ListContainer from './ListContainer'\r\n\timport Recommend from './Recommend'\r\n\timport Floor from './Floor'\r\n\timport Brand from './Brand'\r\n\timport { mapState } from 'vuex'\r\n\r\n\texport default {\r\n\t\tname:'Home',\r\n\t\tcomponents:{ListContainer,Recommend,Floor,Brand},\r\n\t\tcomputed:{\r\n\t\t\t// 从vuex中读取floorList（楼层数据）\r\n\t\t\t...mapState('home',['floorList'])\r\n\t\t},\r\n\t\tmounted() {\r\n\t\t\t// Home组件一挂载，就联系getFloorList获取楼层数据，并存入Vuex\r\n\t\t\tthis.$store.dispatch('home/getFloorList')\r\n\t\t},\r\n\t}\r\n</script>"],"mappings":"AAgBA,OAAAA,aAAA;AACA,OAAAC,SAAA;AACA,OAAAC,KAAA;AACA,OAAAC,KAAA;AACA,SAAAC,QAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IAAAN,aAAA;IAAAC,SAAA;IAAAC,KAAA;IAAAC;EAAA;EACAI,QAAA;IACA;IACA,GAAAH,QAAA;EACA;EACAI,QAAA;IACA;IACA,KAAAC,MAAA,CAAAC,QAAA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}